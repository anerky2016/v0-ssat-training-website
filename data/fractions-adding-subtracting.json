  "vocabulary": [
    {
      "term": "Numerator",
      "kid_friendly_definition": "The top number that counts how many pieces you have."
    },
    {
      "term": "Denominator",
      "kid_friendly_definition": "The bottom number that tells how many equal pieces make a whole."
    },
    {
      "term": "Least Common Denominator (LCD)",
      "kid_friendly_definition": "The smallest bottom number both fractions can share."
    },
    {
      "term": "Simplify",
      "kid_friendly_definition": "Make a fraction smaller but equal by dividing the top and bottom by the same number."
    }
  ]
